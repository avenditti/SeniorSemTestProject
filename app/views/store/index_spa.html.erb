<% if notice %>
    <aside id="notice"><%= notice %></aside>
<% end %>
<% if account_signed_in? and current_account.accountable_type == "Seller" %>
 	<%= react_component("Catalog", seller: true, cart_id: @cart.id, prerender: false) %>
<% else %>
 	<%= react_component("Catalog", seller: false, cart_id: @cart.id, prerender: false) %>
<% end %>